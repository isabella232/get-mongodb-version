{
  "name": "get-mongodb-version",
  "version": "0.0.1",
  "description": "Get the MongoDB version for a path or uri.",
  "scripts": {
    "check": "mongodb-js-precommit",
    "test": "mocha",
    "ci": "./node_modules/istanbul/lib/cli.js cover _mocha -- -R spec ./test/*.test.js"
  },
  "bin": {
    "get-mongodb-version": "bin/get-mongodb-version.js"
  },
  "check": {
    "ignore": [
      "coverage/{*,**/*}"
    ]
  },
  "precommit": [
    "check",
    "test"
  ],
  "dependencies": {
    "debug": "^2.2.0",
    "lodash.startswith": "^4.2.1",
    "minimist": "^1.1.1",
    "mongodb": "^2.0.39",
    "which": "^1.1.1"
  },
  "devDependencies": {
    "coveralls": "^2.11.2",
    "eslint-config-mongodb-js": "^2.2.0",
    "istanbul": "^0.4.5",
    "mocha": "~3.1.2",
    "mongodb-js-precommit": "^0.2.9",
    "pre-commit": "^1.0.10"
  },
  "keywords": [
    "mongodb",
    "mongodb.js"
  ],
  "repository": {
    "type": "git",
    "url": "http://github.com/mongodb-js/get-mongodb-version.git"
  },
  "author": "Lucas Hrabovsky <hrabovsky.lucas@gmail.com> (http://imlucas.com)",
  "license": "Apache-2.0",
  "homepage": "http://github.com/mongodb-js/get-mongodb-version"
}
